{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "lib": [
            "dom",
            "es2015",
            "es2019"
        ],
        "rootDir": "./",
        "downlevelIteration": true,
        "strict": true,
        "types": [
            "node"
        ],
        "moduleResolution": "node",
        "noImplicitAny": false,
        "esModuleInterop": true,
        "removeComments": true,
        "skipLibCheck": true
    }
}