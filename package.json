{
    "name": "@kiko240/cds2types",
    "version": "2.7.4",
    "description": "CLI to convert CDS models to Typescript interfaces and enumerations",
    "main": "./bin/cli.js",
    "repository": "git@github.com:kiko240/cds2types.git",
    "author": "kiko240 <kiko-24@hotmail.es>",
    "license": "MIT",
    "readme": "README.md",
    "bugs": {
        "url": "https://github.com/kiko240/cds2types/issues"
    },
    "bin": {
        "cds2types": "./bin/cli.js"
    },
    "files": [
        "bin"
    ],
    "scripts": {
        "start": "node",
        "build": "tsc --project tsconfig.build.json",
        "test": "tsc --project tsconfig.test.json",
        "test:compileCDS": "ts-node ./src/cli.ts --cds ./test/srv/service.cds --output ./test/service.ts --prefix I --format --sort --json --debug",
        "build:start": "yarn build && yarn start",
        "build:link": "yarn build && npm link"
    },
    "devDependencies": {
        "@commitlint/cli": "^15.0.0",
        "@commitlint/config-conventional": "^15.0.0",
        "@types/eslint": "^8.2.1",
        "@types/fs-extra": "^9.0.13",
        "@types/lodash": "^4.14.178",
        "@types/node": "^14.17.34",
        "@types/prettier": "^2.4.2",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "eslint": "^8.5.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.3",
        "ts-node": "^10.4.0",
        "typescript": "^3.6.4"
    },
    "dependencies": {
        "@sap/cds": "^5.7.3",
        "commander": "^8.3.0",
        "fs-extra": "^10.0.0",
        "lodash": "^4.17.21",
        "prettier": "^2.5.1",
        "ts-morph": "^9.1.0"
    },
    "lint-staged": {
        "*.{js,ts,css,json,yml}": [
            "prettier --write"
        ],
        "*.{js,ts}": [
            "eslint --fix"
        ]
    }
}
